{
    "manifest_version": 3,
    "name": "Sports Score Ticker",
    "version": "1.0",
    "description": "Displays a persistent sports score ticker with fake scores.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "icons": {
      "48": "icons/icon48.png"
    }
  }